@page "/"
@inject UserService userService;
@attribute [Authorize];

<PageTitle>Home</PageTitle>

<h1>Hello, world!</h1>

Welcome to your new app.

<AuthorizeView>
    <Authorized>
        <h1>123</h1>
    </Authorized>
</AuthorizeView>

@foreach(var user in userService.getAll())
{
    <p>@user.Firstname</p>
}